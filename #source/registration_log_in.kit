<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Innovative Medtech</title>
    <link rel="shortcut icon" href="img/icons/favicon.svg" />
    <link rel="stylesheet" href="lips/fontawesome/all.min.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />

    <link rel="stylesheet" href="css/slick.css" />
    <link rel="stylesheet" href="css/swiper-bundle.min.css" />
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <!-- @import "registration_header.html" -->

    <div class="registration">
      <div class="container">
        <div class="registration__container">
          <div class="registration__left">
            <a href="index.html" class="regitration__logo">
              <img src="img/logos/header/Header_Logo_Medtech.svg" alt="" />
            </a>
            <form action="" class="registration__form">
              <h3 class="registration__form__title">
                Войдите в личный кабинет
              </h3>
              <a href="" class="link"> <span>или</span> создайте новый</a>
              <div class="custom__placeholder">
                <p class="placeholder__text">Введите ваш e-mail</p>
                <input class="custom__input" required type="email" />
              </div>
              <input required type="password" placeholder="Введите пароль" />
              <button class="regist__btn">Войти</button>
              <a href="" class="forgot__password">Я забыл пароль</a>
            </form>
          </div>
          <div class="registration__right">
            <div class="img">
              <img src="img/registration__page/surgery.jpg" alt="" />
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="js/jq/jquery.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/swiper-bundle.min.js"></script>
    <script src="js/script.js"></script>
  </body>
</html>

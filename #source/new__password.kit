  <body>
    <!-- @import "registration_header.html" -->

    <div class="registration">
      <div class="container">
        <div class="registration__container dots">
          <div class="registration__dots">
            <div class="registration__dots__block">
              <ul>
                <li>
                  <a href="reset_the_password_mail.html" class="dots__item"
                    >•</a
                  >
                </li>
                <li>
                  <a href="" class="dots__item">•</a>
                </li>
                <li>
                  <a href="" class="dots__item active">•</a>
                </li>
                <li>
                  <a href="" class="dots__item">•</a>
                </li>
              </ul>

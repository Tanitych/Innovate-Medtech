<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Innovative Medtech</title>
    <link rel="shortcut icon" href="img/icons/favicon.svg" />
    <link rel="stylesheet" href="lips/fontawesome/all.min.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />

    <link rel="stylesheet" href="css/slick.css" />
    <link rel="stylesheet" href="css/swiper-bundle.min.css" />
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <!-- @import "registration_header.html" -->

    <div class="registration">
      <div class="container">
        <div class="registration__container dots">
          <div class="registration__dots">
            <div class="registration__dots__block">
              <ul>
                <li>
                  <a href="registration_role.html" class="dots__item">•</a>
                </li>
                <li>
                  <a href="pacient_regist_fill_data.html" class="dots__item"
                    >•</a
                  >
                </li>
                <li><a href="" class="dots__item active">•</a></li>
              </ul>
            </div>
          </div>
          <div class="registration__left">
            <a href="index.html" class="regitration__logo">
              <img src="img/logos/header/Header_Logo_Medtech.svg" alt="" />
            </a>
            <!-- <p class="create___acc">Создание аккаунта для пациента</p> -->
            <form action="" class="registration__form role">
              <div class="registration__form__content">
                <p class="create___acc mb_less">
                  Создание аккаунта для пациента
                </p>
                <h3 class="registration__form__title add_margin">
                  Заполните ваш профиль
                </h3>
                <div class="img__avatar__body">
                  <div class="img__avatar_block">
                    <img
                      src="img/pacient_regist_fill_profile/empty_avatar.jpg"
                      alt=""
                      class="avatar_img"
                    />
                  </div>
                </div>
                <div class="buttons__block">
                  <input required type="text" placeholder="Ваш Ф.И.О" />
                  <input
                    id="phone"
                    required
                    type="tel"
                    placeholder="Ваш номер телефона"
                  />
                  <button class="next__link save__btn">Сохранить</button>
                </div>
              </div>
            </form>
          </div>
          <div class="registration__right">
            <div class="img">
              <img src="img/registration__page/surgery.jpg" alt="" />
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="js/jq/jquery.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/swiper-bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.4.1/jquery.maskedinput.min.js"></script>
    <script src="js/script.js"></script>
  </body>
</html>
